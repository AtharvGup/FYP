{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "GCN",
    "dataset": "taowu_schaefer100",
    
    "out_dir": "out/braindata_graph_classification/",

    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 20,
        "init_lr": 0.00021888619663268843,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-6,
        "weight_decay":4.777074196333867e-05,
        "print_epoch_interval": 5,
        "max_time": 12
    },

    "net_params": {
        "L": 3,
        "hidden_dim": 128,
        "out_dim": 128,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "self_loop": false,
        "edge_feat": true
    }	
}